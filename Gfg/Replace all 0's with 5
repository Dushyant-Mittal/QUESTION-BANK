You are given an integer n. You need to convert all zeroes of n to 5.


//*you are required to complete this method*/
class Solution {
  public:
    int convertFive(int n) {
        // Your code here
        string str=to_string(n);
        for(char &c:str){
            if(c=='0'){
                c='5';
            }
        }
        return stoi(str);
    }
};
