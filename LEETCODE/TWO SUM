class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target){
        vector<int>ans;
        int n=nums.size();
        int x,y;
        for(int i=0; i<n; i++){
            for(int j=i+1; j<n; j++){
                if(nums[i]+nums[j] == target){
                    x=i;
                    y=j;    
                }
            }
        }
        ans.push_back(x);
        ans.push_back(y);
        return ans;
    }
};
